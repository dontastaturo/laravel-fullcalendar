{
    "name": "dontastaturo/laravel-fullcalendar",
    "description": "Laravel component for fullcalendar package",
    "keywords": [
        "fullcalendar",
        "javascript",
        "event",
        "calendar",
        "laravel"
    ],
    "homepage": "https://github.com/edofre/laravel-fullcalendar",
    "license": "MIT",
    "authors": [{
        "name": "Edo Freriks",
        "email": "edofre@gmail.com"
    }],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^6.0",
        "npm-asset/fullcalendar": "v4.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Edofre\\Fullcalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edofre\\Fullcalendar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Edofre\\Fullcalendar\\FullcalendarServiceProvider"
            ],
            "aliases": {
                "Fullcalendar": "Edofre\\Fullcalendar\\Facades\\Fullcalendar"
            }
        }
    }
}